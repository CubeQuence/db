{
    "name": "cubequence/db",
    "description": "Database handling for CubeQuence",
    "keywords": [
        "database",
        "cubequence"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Luca Castelnuovo",
            "email": "luca@castelnuovo.xyz"
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "catfan/medoo": "^2.0",
        "cubequence/helpers": "^1.0",
        "robmorgan/phinx": "^0.12.6"
    },
    "require-dev": {
        "fakerphp/faker": "^1.14",
        "nunomaduro/phpinsights": "dev-master",
        "pestphp/pest": "^1.0"
    },
    "scripts": {
        "tests": "./vendor/bin/pest"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "CQ\\DB\\": "src/DB"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "issues": "https://github.com/CubeQuence/db/issues",
        "source": "https://github.com/CubeQuence/db"
    }
}
